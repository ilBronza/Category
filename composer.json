{
    "name": "ilbronza/category",
    "description": "category manager",
    "license": "license",
    "authors": [
        {
            "name": "Davide Bronzini",
            "email": "davide@sistema.srl",
            "homepage": "https://www.sistema.srl"
        }
    ],
    "homepage": "https://github.com/ilbronza/category",
    "keywords": ["Laravel", "Category"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "ilBronza\\Category\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ilBronza\\Category\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ilBronza\\Category\\CategoryServiceProvider"
            ],
            "aliases": {
                "Category": "ilBronza\\Category\\Facades\\Category"
            }
        }
    }
}
